<section id="portfolio" class="single-page scrollblock">
  <div class="container">
    <div class="align"><%= image_tag("like.png")%></div>
    <h1 id="folio-headline">Instituições Recomendadas</h1>
    <div class="row">
      <% if @pages[0].present?%>
      <div class="span4">
        <div class="mask2" style="height:224px; width:377px; transform: translateY( 25% );"> <img src="<%= @pages[0].logo.logo_full.url%>" alt=""></div>
        <div class="inside">
          <hgroup>
            <h2><b><%= @pages[0].user.name_institute %></b></h2>
          </hgroup>
          <div class="entry-content">
            <p><%= @pages[0].recommends %> pessoas recomendaram</p>
            </div>
        </div>
      </div>
      <% end %>
      <% if @pages[1].present? %>
      <div class="span4">
        <div class="mask2" style="height:224px; width:377px; transform: translateY( 25% );"> <img src="<%= @pages[1].logo_url%>" alt=""></div>
        <div class="inside">
          <hgroup>
            <h2><b><%= @pages[1].user.name_institute %></b></h2>
          </hgroup>
          <div class="entry-content">
           <p><%= @pages[1].recommends %> pessoas recomendaram</p>
            </div>
        </div>
      </div>
      <% end %>
      <% if @pages[2].present? %>
      <div class="span4">
        <div class="mask2" style="height:224px; width:377px; transform: translateY( 25% );"> <img src="<%= @pages[2].logo_url%>" alt=""></div>
        <div class="inside">
          <hgroup>
            <h2><b><%= @pages[2].user.name_institute %></b></h2>
          </hgroup>
          <div class="entry-content">
            <p><%= @pages[2].recommends %> pessoas recomendaram</p>
            </div>
        </div>
      </div>
      <% end %>
    </div>
    
  </div>
</section>
