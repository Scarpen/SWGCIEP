<div class="container-fluid">
  <div class="row row-offcanvas row-offcanvas-left">
    <div class="col-md-12">
      <h2 style="text-align: center;"> Gerenciar Menu </h2>
        <%= simple_form_for @page do |f| %>  
        <%= image_tag(@page.logo) if @page.logo? %>
          <%= f.input :logo %>
          <p><%= f.submit "Submit" %></p>
        <% end %>
    </div>
  </div>
</div>