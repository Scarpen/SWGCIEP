<div class="panel panel-primary">
<div class="container-fluid">
  <div class="row row-offcanvas row-offcanvas-left">
    <div class="col-md-12">
       <h1 style="margin-top:40px;"><b>Alterar Logo</b></h1>
        <%= simple_form_for @page do |f| %>  
        <%= image_tag(@page.logo) if @page.logo? %>
          <%= f.input :logo, label: false %>
          <center><p><%= f.submit "Salvar", class: "btn btn-lg btn-success btn-block", style:"height:45px; width:130px;" %></p></center>
        <% end %>
    </div>
  </div>
</div>
